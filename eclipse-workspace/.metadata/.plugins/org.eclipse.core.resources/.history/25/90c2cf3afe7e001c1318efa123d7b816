package com.map;

import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;

public class MapDemo {
	
	public static void main(String[] args) {
		
		
		Map<Integer , Character> hm = new HashMap<Integer , Character>();
		
		hm.put(1 , 'j');
		hm.put(2 , 's');
		hm.put(3 , 'r');

		//System.out.println(hm);
		
		for(Entry<Integer, Character> a : hm.entrySet()) {
			
			System.out.println(a.getKey());
			System.out.println(a.getValue());
		}
		
		Map<Character , Integer> h = new HashMap<Character , Integer>();
		h.put('a', 1);
		h.put('b', 1);
		h.put('c', 3);
		h.put('d', 4);
		h.put('e', 5);
		h.put('f', 5);
		h.put('g', 5);
		
		
	}
	
	
	public static void repeat(Map<Character , Integer> a) {
		
		Map<Character , Integer> b = new HashMap<Character , Integer>();
		
		
		for(int i=0 ; i<a.size(); i++) {
			
			b=a;
			
		}
		
		System.out.println(((Entry<Integer, Character>) b).getValue());
	}

}
