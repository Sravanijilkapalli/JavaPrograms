package com.map;

import java.util.HashMap;
import java.util.Map;

public class MapDemo {
	
	public static void main(String[] args) {
		
		
		Map<Integer , Character> hm = new HashMap<Integer , Character>();
		
		hm.put(1 , 'j');
		hm.put(2 , 's');
		hm.put(3 , 'r');

		System.out.println(hm);
		
		
		
	}

}
