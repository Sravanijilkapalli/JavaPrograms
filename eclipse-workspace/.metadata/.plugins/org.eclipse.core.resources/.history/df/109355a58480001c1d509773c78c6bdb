package exception;

public class ExceptionDemo {
	
	public static void main(String[] args) {
		
		
		
		ExceptionDemo obj = null;
		
		try {
			obj.demo();
		}catch(Exception e) {
			System.out.println(raja);
		}
		
	
		
	}
	
	public int demo() {
		
		int a = 3;
		int b = 0;
		
		try {
			int c = a/b;
			System.out.println(c);
		}catch(Exception e) {
			throw e;
			
		}
		
		int d = a*b;
		
		System.out.println(d);
		
		return 0;
	}

}
